                </div>

                <div class="rect">
                    <h4>100</h4>
                    <p>Clients</p>
                </div>
                <div class="rect">
                    <h4>100</h4>
                    <p>Employees</p>
                </div>
                <!-- <div class="rect">
                    <h4>name</h4>
                    <p>6</p>
                </div> -->
            </div>
        </div>
    </section>

    <!-- gallary -->

    <section class="mb-5">
        <div class="container">
            <h2 class="card-h1">Equipments Gallary</h2>
            <div class="section-title">
                <div class="services-devider" />
            </div>
            <div>
                <div class="cr-wrapper text-center">
                    <div class=" box1 text-center">
                        <div class="img-container text-center">
                            <img class="img1-a" />
                            <a href="/media" class="room-link btnprimary">View All</a>
                        </div>
                    </div>
                    <div class=" box box5">
                        <div class="img-container  text-center">
                            <img class="img1-b" />

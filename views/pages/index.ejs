<%- include('../partials/header')%>

    <!-- showcase slider -->

    <section id="showcase">
        <div id="myCourosel1" class="carousel slide bg-carousel carousel-fade" style=" transition: all 2s ease 1s;">
            <ol class="carousel-indicators">
                <li data-target="#myCourosel1" data-slide-to="0" class="active"></li>
                <li data-target="#myCourosel1" data-slide-to="1"></li>
                <li data-target="#myCourosel1" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner">
                <div class="carousel-item  active">
                    <img class="img-fluid carousel-image-1" />
                    <div class="container">
                        <div class="carousel-caption   text-center corousel-text mb-5">
                            <h1 class="display-3"> R.K. TRANSPORT <br />&<br /><span>RADHA</span>
                                <span>KRISHNA</span> <span>CRANES</span>
                            </h1>

                            <a href="/contact-us" style="text-decoration: none;" class="btnprimary">Contact us</a>
                        </div>
                    </div>
                </div>
                <div class="carousel-item ">
                    <img class="img-fluid carousel-image-2" />

                    <div class="container">
                        <div class="carousel-caption  corousel-text">
                            <h1 class="display-3"> R.K. TRANSPORT <br />&<br /><span>RADHA</span>
                                <span>KRISHNA</span> <span>CRANES</span>
                            </h1>


                            <a href="/contact-us" style="text-decoration: none;" class="btnprimary">Contact us</a>
                        </div>
                    </div>
                </div>
                <div class="carousel-item ">
                    <img class="img-fluid carousel-image-3" />

                    <div class="container">
                        <div class="carousel-caption  text-center corousel-text mb-5">
                            <h1 class="display-3"> R.K. TRANSPORT <br />&<br /><span>RADHA</span>
                                <span>KRISHNA</span> <span>CRANES</span>
                            </h1>


                            <a href="/contact-us" style="text-decoration: none;" class="btnprimary">Contact us</a>
                        </div>
                    </div>
                </div>
            </div>
            <a class="carousel-control-prev" href="#myCourosel1" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#myCourosel1" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </section>

    <!-- // animation -->

    <section class="roadreqscene animation-display">
        <img class="mancrane" src="/images/animation/manrane.png" alt="image not found1">
        <img class="scisorlift" src="/images/animation/scisorlift.png" alt="image not found2">
        <img class="reqcrane" src="/images/animation/crane.png" alt="image not found3">
        <img class="Forklift" src="/images/animation/forklift.png" alt="image not found4">
        <img class="anitruck" src="/images/animation/animatedtruck.png" alt="image not found5">
        <img class="box1-anim" src="/images/animation/BOX.png" alt="image not found6">
        <img class="box2-anim" src="/images/animation/BOX.png" alt="image not found7">
        <h1 class="anitrucktitle">RK transport</h1>


    </section>
    <!-- services -->

    <section class="my-5">
        <div class="wrapper">
            <h2 class="card-h1">Our Services</h2>
            <div class="section-title">
                <div class="services-devider" />
            </div>
            <div class="cols">
                <div class="owl-carousel cr-service-corousel owl-theme">
                    <% services?.forEach(function(service) { %>
                        <div class="col3 ml-2" ontouchstart="this.classList.toggle('hover');">
                            <div class="container2">
                                <div class="front" style="background-image: url('<%= service?.image %>')">
                                    <div class="inner">
                                        <p>
                                            <%= service?.title %>
                                        </p>
                                    </div>
                                </div>
                                <div class="back">
                                    <div class="inner">
                                        <p class="p-size">
                                            <%= service?.s_dec %>
                                        </p>
                                        <h5><a href="/services?id=<%= service?.service_id %>" class="read-more">Read
                                                more
                                                <i class="fa fa-angle-double-right" aria-hidden="true"></i></a></h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <% }) %>
                </div>
            </div>
        </div>
    </section>

    <!-- servies -->


    <!-- video play -->
    <section id="video-play">
        <div class="dark-overlay">
            <div class="row">
                <div class="col">
                    <!-- <div class="container p-5">
                        <a href="#" class="video " data-video="https://www.youtube.com/embed/pxoIufVFjOk"
                            data-toggle="modal" data-target="#videoModal">
                            <i class="fab fa-youtube-square"></i>
                        </a>
                        <!-- <h1>See what we do</h1> -->
                </div> -->
            </div>
        </div>
        </div>
    </section>

    <!-- equipments -->

    <section class="equipment-margin">
        <h2 class="card-h1">Our Equipments</h2>
        <div class="section-title">
            <div class="services-devider" />
        </div>
        <div class="container">
            <div class="owl-carousel cr-equipment-carousel owl-theme">
                <% infras?.forEach((category)=> { %>
                    <div class="m-3 d-flex flex-column justify-content-center align-items-center">
                        <a href="/infrastructure/cranes?categoryid=<%= category?.infra_id%>" class="text-center"><img
                                src="<%= category?.image %>"></a>
                        <h5 class="text-center m-3">
                            <%= category?.name %>
                        </h5>
                    </div>
                    <% }); %>
            </div>
        </div>

    </section>

    <!-- counter -->

    <section class="pb-5">
        <div class="cover">
            <div class="overlay"></div>
            <div class="content text-center">
                <h2>Some Features That Made Us Unique</h3>

            </div>
        </div>
        <div class="container-fluid text-center">
            <div class="numbers d-flex flex-md-row flex-wrap justify-content-center">

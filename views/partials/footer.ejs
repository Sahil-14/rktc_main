<footer>
  <div class="container px-5 pt-5 pb-4">
    <div class="row">
      <div class="col-lg-3 col-md-6 col-sm-12">
        <div class="footer-widget about-widget mb-5 d-flex align-items-center justify-content-center">
          <img src="/images/rktc logo.png" width="150" height="150" class="logo" />
        </div>
      </div>
      <div class="col-lg-3 col-md-6 col-sm-12">
        <div class="footer-widget services-widget">
          <h2 class="widget-title">Services</h2>
          <ul>
            <% if (footerServices) { %>
              <% footerServices?.forEach(service=> { %>
                <li><a href="/services?id=<%= service?.service_id %>">
                    <%= service?.title %>
                  </a></li>
                <% }); %>
                  <% } %>
          </ul>
        </div>
      </div>
      <div class="col-lg-3 col-md-6 col-sm-12">
        <div class="footer-widget information-widget">
          <h2 class="widget-title">Information</h2>
          <ul>
            <li><a href="/aboutus">About us</a></li>
            <li><a href="/contact-us">Conatact us</a></li>
            <li><a href="/media">Media</a></li>
            <li><a href="/infrastructure/cranes">Infrastructure</a></li>
          </ul>
        </div>
      </div>

      <div class="col-lg-3 col-md-6 col-sm-12">
        <div class="footer-widget">
          <h2 class="widget-title">Contact</h2>
          <div class="contact-address">
            <i class="fas fa-map-marker-alt" aria-hidden="true"></i> Address:
            Sr. No 16,Vitthal
            Prasad,At-Bhilarewadi,Post-Katraj,Tal.Haveli,Pune-411046
          </div>
          <div class="contact-number">
            <i class="fa fa-phone" aria-hidden="true"></i> Phone : +91
            7620545354
          </div>
          <div class="contact-email">
            <i class="fas fa-envelope"></i> Email: info@rktcind.com
          </div>
          <div class="social-widget">
            <a href=""><i class="fab fa-facebook-square"></i></a>
            <a href=""><i class="fab fa-google-plus-square"></i></a>

            <a href=""><i class="fab fa-twitter-square"></i></a>
            <a href=""><i class="fab fa-youtube-square"></i></a>
            <a href=""><i class="fab fa-instagram"></i></a>
          </div>
        </div>
      </div>
    </div>

  </div>
  <% var year=new Date().getFullYear()%>
    <div class="copyright">
      <div class="container d-flex justify-content-center align-items-center mx-auto flex-column">
        <p>copyright &copy; <%= year%> RKTC</p>

      </div>
    </div>
</footer>